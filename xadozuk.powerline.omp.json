{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [


    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#eeeeee",
          "foreground_templates": [
            "{{ if gt .Code 0 }}red{{ end }}"
          ],
          "properties": {
            "always_enabled": false
          },
          "style": "diamond",
          "template": "\uf7d3 ",
          "type": "exit"
        },
        {
          "foreground": "#eeeeee",
          "style": "plain",
          "template": "\u276f",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
