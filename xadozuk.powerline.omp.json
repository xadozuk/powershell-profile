{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none",
            "style": "round"
          },
          "template": " {{ .FormattedMs }} ",
          "foreground": "#eeeeee",
          "background": "green",
          "type": "executiontime",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "source": "pwsh"
          },
          "alias": "AzPwsh",
          "template": " \uebd8 {{ .Name }}",
          "foreground": "#eeeeee",
          "powerline_symbol": "\ue0b0",
          "background": "blue",
          "type": "az",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ if .Segments.AzPwsh.Name }}|{{ end }}",
          "foreground": "lightBlue",
          "powerline_symbol": "\ue0b0",
          "background": "blue",
          "type": "text",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "source": "cli"
          },
          "template": "\udb82\udc05 {{ .Name }} ",
          "foreground": "#eeeeee",
          "powerline_symbol": "\ue0b0",
          "background": "blue",
          "type": "az",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "parse_kubeconfig": true
          },
          "template": " \uf308 {{ .Context }}{{ if .Namespace }} :: {{ .Namespace }}{{ end }} ",
          "foreground": "#eeeeee",
          "powerline_symbol": "\ue0b0",
          "background": "#c19c00",
          "type": "kubectl",
          "style": "powerline"
        },
        {
          "properties": {
            "branch_icon": "\ue725 ",
            "branch_max_length": 0,
            "cache_duration": "none",
            "fetch_stash_count": false,
            "fetch_status": false,
            "fetch_upstream_icon": false
          },
          "template": " {{ .HEAD }} ",
          "foreground": "black",
          "powerline_symbol": "\ue0b0",
          "background": "lightCyan",
          "type": "git",
          "style": "powerline"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "home_icon": "~",
            "style": "full"
          },
          "template": "\uf07b {{ .Path }} ",
          "foreground": "darkGray",
          "background": "transparent",
          "type": "path",
          "style": "plain"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": false,
            "cache_duration": "none"
          },
          "template": "\udb80\uded4 ",
          "foreground": "#eeeeee",
          "type": "exit",
          "style": "diamond",
          "foreground_templates": [
            "{{ if gt .Code 0 }}red{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "‚ùØ",
          "foreground": "#eeeeee",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
